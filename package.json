{
  "name": "procura-cep",
  "description": "A tool for reverse lookup of a Brazillian address using its CEP (Código de endereçamento postal). It fetches from https://viacep.com.br (shoutouts for over 5 years of service!)",
  "contributors": [
    {
      "name": "Thiago Cruz Santos",
      "email": "thiagocruzsantos@gmail.com"
    }
  ],
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "host": "vite dev --host",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@imask/svelte": "^6.4.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  },
  "type": "module"
}
